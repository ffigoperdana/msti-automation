# Environment untuk VPS MSTI Automation
# Copy file ini ke /opt/msti-automation/.env di VPS Anda

# Docker Registry - sesuaikan dengan Docker Hub Anda
DOCKER_USERNAME=your-dockerhub-username
IMAGE_TAG=latest

# Database Configuration
DATABASE_URL=postgresql://msti_user:msti_password@postgres:5432/msti_automation
POSTGRES_DB=msti_automation
POSTGRES_USER=msti_user
POSTGRES_PASSWORD=msti_password

# InfluxDB Configuration - sesuaikan dengan InfluxDB Anda
INFLUX_URL=http://192.168.238.10:8086
INFLUX_TOKEN=your-influx-token

# Application Configuration
NODE_ENV=production
APPLY_MIGRATIONS=false
PORT=3001
WEBHOOK_PORT=3002

# Deployment Options
KEEP_OLD_ENV=false
DEPLOYMENT_TIMESTAMP= 